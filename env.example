# Required for voice agent models
OPENAI_API_KEY=
DEEPGRAM_API_KEY=
CARTESIA_API_KEY=

# Only needed for local `dev` / `start` connecting to LiveKit
LIVEKIT_URL=
LIVEKIT_API_KEY=
LIVEKIT_API_SECRET=

# Public URL of your Node server (used by the agent to post per-call metrics)
# Example: https://your-domain.com
SERVER_BASE_URL=

# Shared secret to authenticate the agent to the server internal endpoints.
# Must match server's AGENT_SHARED_SECRET.
AGENT_SHARED_SECRET=

# Optional: Set an explicit LiveKit Agent name so Telephony Dispatch Rules / Playground can target this agent.
# If empty, LiveKit will not show a name and you cannot explicitly dispatch it by name.
LIVEKIT_AGENT_NAME=


